# Google OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# API Configuration
VITE_API_BASE_URL=http://localhost:3000

# Development settings
# Set to 1 for development mode with fake login buttons
# Set to 0 for production mode with Google OAuth only
VITE_DEV_AUTH=0

# OAuth Redirect URI (should match your Google OAuth settings)
VITE_GOOGLE_REDIRECT_URI=http://localhost:3000/auth/callback